import numpy as np

def dot_product(array1, array2):
  dot_product = 0
  for i in range(len(array1)):
    dot_product += array1[i] * array2[i]

  return dot_product


array1 = np.array([1, 2, 3])
array2 = np.array([4, 5, 6])

dot_product = dot_product(array1, array2)

print("Dot product:", dot_product)
